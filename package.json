{
  "name": "unistack",
  "version": "0.0.1",
  "description": "An example of unistack",
  "private": true,
  "scripts": {
    "start": "./start",
    "pretest": "npm install && node_modules/node-sass/bin/node-sass app/scss/style.scss --output .generated && NODE_ENV=production node_modules/browserify/bin/cmd.js --entry app/client.js > .generated/bundle.js",
    "test": "./node_modules/.bin/mocha --delay --compilers js:babel/register test/test.js"
  },
  "license": "MIT",
  "dependencies": {
    "babel": "5.8.21",
    "babelify": "6.2.0",
    "baconjs": "0.7.71",
    "bluebird": "2.9.34",
    "browserify": "11.0.1",
    "colors": "0.6.2",
    "compression": "1.5.2",
    "express": "4.13.3",
    "forever": "0.15.1",
    "node-sass": "3.3.3",
    "react": "0.14.0",
    "react-dom": "0.14.0",
    "uglify-js": "2.4.19"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "mocha": "2.3.3",
    "nodemon": "1.3.6",
    "supervisor": "0.6.0",
    "watchify": "3.1.0"  ,
    "zombie": "4.1.0"
  }
}
